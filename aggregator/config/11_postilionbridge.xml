<server name="postilionSink" class="org.jpos.q2.iso.QServer" logger="postilionLogger">
    <attr name="port" type="java.lang.Integer">8899</attr>
    <channel name="postilionSink" class="org.jpos.iso.channel.PostChannel" 
             packager="org.jpos.iso.packager.PostPackager" logger="postilionLogger">
        <property name="packager-config" value="aggregator/pack/pospack.xml"/>
        <property name="timeout" value="300000"/>
    </channel>
    <request-listener class="com.amecore.bridge.aggregator.TXNListener" logger="postilionLogger" realm="postilionSink"/>
</server>