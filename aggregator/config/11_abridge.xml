<server name="aggregator" class="org.jpos.q2.iso.QServer" logger="aggLogs">
	<attr name="port" type="java.lang.Integer">8888</attr>
	<channel name="aggegator" class="org.jpos.iso.channel.PostChannel" 
		packager="org.jpos.iso.packager.PostPackager" logger="aggLogs">
		<property name="packager-config" value="aggregator/pack/pospack.xml"/>
		<property name="timeout" value="300000"/>
	</channel>
	<request-listener class="com.amecore.bridge.aggregator.TXNListener" logger="aggregatorLogs"/>
</server>